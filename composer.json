{
  "name": "treehouselabs/recommendation-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle that implements a recommendation engine in your project",
  "keywords": [],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Kruithof",
      "email": "peter@treehouse.nl",
      "role": "Lead developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "psr/log": "^1.0",
    "php-http/curl-client": "^1.1",
    "php-http/message": "^1.0",
    "guzzlehttp/psr7": "^1.0",
    "treehouselabs/cache": "^2.0",
    "symfony/symfony": "^2.7|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "php-http/mock-client": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\RecommendationBundle\\": "src/TreeHouse/RecommendationBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TreeHouse\\RecommendationBundle\\Tests\\": "tests/TreeHouse/RecommendationBundle/Tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
